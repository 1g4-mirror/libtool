  srcdir=`echo "$0" | sed 's%/[^/]*$%%'`
  test "$srcdir" = "$0" && srcdir=.
fi
. $srcdir/defs || exit 1

func_mkprefixdir
func_cd "f77demo"
func_make_distclean
func_configure
func_check_static_shared "yes" "yes"

exit 0
