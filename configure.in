dnl Process this file with autoconf to create configure.
AC_INIT(ltmain.in)
dnl $Format "AM_INIT_AUTOMAKE($ProjectName$,$ProjectMajorVersion: 1.0g $"$
AM_INIT_AUTOMAKE(libtool,1.0g)

pkgdatadir='${datadir}/libtool'
AC_SUBST(pkgdatadir)
aclocaldir='${datadir}/aclocal'
AC_SUBST(aclocaldir)

dnl Use the specified CC, RANLIB, and LD while running ltconfig.
AC_PROG_CC
AC_PROG_RANLIB
AM_PROG_LD
AM_PROG_NM
AC_PROG_LN_S

dnl For lineno
AC_PROG_AWK

AC_OUTPUT([Makefile doc/Makefile tests/Makefile])

